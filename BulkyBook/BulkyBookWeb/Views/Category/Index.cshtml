@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>
<table class="table">
  <a asp-controller="Category" asp-action="Create" class="btn btn-primary">Insert</a>
  <thead>
    <tr>
      <th scope="col">Category Name</th>
      <th scope="col">Category Order</th>

    </tr>
  </thead>
  <tbody>
        @foreach(var obj in Model) 
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.DisplayOrder
                </td>
            </tr>
          
        }
       
   
  </tbody>
</table>


